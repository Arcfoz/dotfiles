{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.30/schema.json",
  "app_specific_configuration_path": "$Env:KOMOREBI_CONFIG_HOME/applications.json",
  "window_hiding_behaviour": "Cloak",
  "cross_monitor_move_behaviour": "Insert",
  "default_workspace_padding": 8,
  "default_container_padding": 8,
  "mouse_follows_focus": false,
  "border": true,
  "border_width": 5,
  "theme": {
    "palette": "Base16",
    "name": "Ashes",
    "unfocused_border": "Base03",
    "bar_accent": "Base0D"
  },
  "stackbar": {
    "height": 40,
    "mode": "OnStack",
    "tabs": {
      "width": 300
    }
  },
  "animation": {
    "enabled": false
  },
  "monitors": [
    {
      "workspaces": [
        {
          "name": "I",
          "layout": "BSP"
        },
        {
          "name": "II",
          "layout": "BSP"
        },
        {
          "name": "III",
          "layout": "BSP"
        },
        {
          "name": "IV",
          "layout": "BSP"
        },
        {
          "name": "V",
          "layout": "BSP"
        }
      ]
    }
  ],
  "ignore_rules": [
    {
      "kind": "Exe",
      "matching_strategy": "Equals",
      "id": "explorer.exe"
    },
    {
      "kind": "Exe",
      "matching_strategy": "Equals",
      "id": "Discord.exe"
    },
    {
      "kind": "Exe",
      "matching_strategy": "Equals",
      "id": "Telegram.exe"
    },
    {
      "kind": "Title",
      "matching_strategy": "Equals",
      "id": "Settings"
    },
    {
      "kind": "Title",
      "matching_strategy": "Equals",
      "id": "Microsoft Store"
    },
    {
      "kind": "Exe",
      "matching_strategy": "Equals",
      "id": "ApplicationFrameHost.exe"
    },
    {
      "kind": "Class",
      "matching_strategy": "Equals",
      "id": "MozillaDialogClass"
    },
    {
      "kind": "Exe",
      "matching_strategy": "Equals",
      "id": "yasb.exe"
    },
    {
      "kind": "Exe",
      "matching_strategy": "Equals",
      "id": "Notepad.exe"
    },
    {
      "kind": "Exe",
      "matching_strategy": "Equals",
      "id": "python3.12.exe"
    },
    {
      "kind": "Exe",
      "matching_strategy": "Equals",
      "id": "Spotify.exe"
    },
    {
      "kind": "Exe",
      "matching_strategy": "Equals",
      "id": "PowerToys.Settings.exe"
    },
    {
      "kind": "Exe",
      "matching_strategy": "Equals",
      "id": "slack.exe"
    },
    {
      "kind": "Exe",
      "matching_strategy": "Equals",
      "id": "WinRAR.exe"
    },
    {
      "kind": "Exe",
      "matching_strategy": "Equals",
      "id": "Photos.exe"
    },
    {
      "kind": "Exe",
      "matching_strategy": "Equals",
      "id": "Mechvibes.exe"
    },
    {
      "kind": "Exe",
      "matching_strategy": "Equals",
      "id": "olk.exe"
    },
    {
      "kind": "Exe",
      "matching_strategy": "Equals",
      "id": "Notion Calendar.exe"
    }
  ]
}
